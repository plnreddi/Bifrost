  "acls": [
  {
    "accessType": "*",
    "principalType": "ROLE",
    "principalId": "admin",
    "permission": "ALLOW"
  }, 
  {
    "accessType": "READ",
    "principalType": "ROLE",
    "principalId": "$unauthenticated",
    "permission": "DENY"
  }, 
  {
    "accessType": "READ",
    "principalType": "ROLE",
    "principalId": "$authenticated",
    "permission": "ALLOW"
  }, 
  {
    "accessType": "*",
    "principalType": "ROLE",
    "principalId": "$owner",
    "permission": "ALLOW"
  }],

  "methods": []
}
