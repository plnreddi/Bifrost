{
  "name": "Doctor",
  "plural": "Doctors",
  "base": "User",
  "idInjection": false,

  "options": {
    "validateUpsert": true
  },

  "properties": {
    "doctorName": {
      "type": "string"
    },
    "doctorPhoto": {
      "type": "string"
    },
    "phoneVerified": {
      "type": "boolean",
      "default": "false"
    },
    "aboutDoctor": {
      "type": [
        "List"
      ]
    },
    "doctorCredentialOne": {
      "type": "List"
    },
    "doctorCredentialTwo": {
      "type": "List"
    },
    "doctorCredentialThree": {
      "type": "List"
    },
    "doctorCredentialFour": {
      "type": "List"
    },
    "doctorCredentialFive": {
      "type": "List"
    },
    "doctorCredentialSix": {
      "type": "List"
    },
    "doctorCredentialSeven": {
      "type": "List"
    },
    "doctorCredentialEight": {
      "type": "List"
    },
    "yearsExperience": {
      "type": "string"
    }
  },

  "validations": [],
  
  "relations": {
    "hospitals": {
      "type": "hasMany",
      "model": "Hospital",
